{
  "version": 2,
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "myproject/wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "myproject/wsgi.py"
    },
    {
      "src": "/favicon.ico",
      "dest": "myproject/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings",
    "PYTHONPATH": "."
  }
}
